#include <stdio.h>

int isPrime(int n) {
    if (n <= 1)
        return 0;
    for (int i = 2; i <= n / 2; i++) {
        if (n % i == 0)
            return 0; }
    return 1;
}
void displayPrimes(int first, int second) {
    printf("Prime numbers between %d and %d are:\n", first, second);
    for (int i = first; i <= second; i++) {
        if (isPrime(i)) {
            printf("%d ", i);
        }
    }
    printf("\n"); }

int main() {
    int num1, num2;
    printf("Enter the first number: ");
    scanf("%d", &num1);
    printf("Enter the Second number: ");
    scanf("%d", &num2);

    displayPrimes(num1, num2);
    return 0;
}
